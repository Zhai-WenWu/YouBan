<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/layout_view_1">


    <TextView
        android:id="@+id/tip_text_view_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="试用商品过后请您在规定时间内发布试"
        android:textSize="@dimen/text_size_16"
        android:textColor="@color/color_333333"/>

    <Button
        android:id="@+id/btn_view_3"
        android:layout_width="89dp"
        android:layout_height="28dp"
        android:layout_below="@+id/tip_text_view_1"
        android:layout_alignParentRight="true"
        android:layout_marginTop="20dp"
        android:background="@drawable/shape_round_10_solid_ffee00"
        android:text="发布试用报告"
        android:textColor="@color/color_333333"
        android:textSize="@dimen/text_size_11"
        android:visibility="visible" />



</RelativeLayout>